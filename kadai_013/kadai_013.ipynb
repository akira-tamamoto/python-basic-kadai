        "#　関数定義\n",
        "def total_price(price, tax):\n",
        "    #　変数totalに消費税計算を代入\n",
        "    tax = tax / 100 * price\n",
        "    total = price + tax\n",
        "    return total\n",
        "\n",
        "total_price(110,10)"
