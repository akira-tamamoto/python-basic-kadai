        "from os import name\n",
        "class Human:\n",
        "    #コンストラクタ\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    #　メソッド定義\n",
        "    def printinfo(self):\n",
        "        print(f\"{self.name},{self.age}\")\n",
        "\n",
        "# インスタンス化\n",
        "human = Human(\"akira\", \"32\")\n",
        "\n",
        "human.printinfo()"
